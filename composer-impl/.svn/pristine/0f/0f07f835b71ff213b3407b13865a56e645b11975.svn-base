<?xml version="1.0" encoding="UTF-8"?>
<tns:service id="UM_UnloadFromGate1" xmlns:tns="http://soa.fbk.eu/Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://soa.fbk.eu/Service ../../schemata/Service.xsd ">
  <tns:states>
    <tns:state isInitial="true">ST0</tns:state>
    <tns:state>ST1</tns:state>
    <tns:state>ST2</tns:state>
     <tns:state>ST3</tns:state>
  </tns:states>
  <tns:actions>
    <tns:input>Unload1_Request</tns:input>
    <tns:output>UnloadFromGate1_Reply</tns:output>
    <tns:output>UnloadFromGate1B_Reply</tns:output>
  </tns:actions>
  <tns:transitions>
    <tns:transition>
      <tns:initialState>ST0</tns:initialState>
      <tns:finalState>ST1</tns:finalState>
      <tns:input>Unload1_Request</tns:input>
    </tns:transition>
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST2</tns:finalState>
      <tns:output>UnloadFromGate1_Reply</tns:output>
    </tns:transition> 
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST3</tns:finalState>
      <tns:output>UnloadFromGate1B_Reply</tns:output>
    </tns:transition>    
  </tns:transitions>
  
  <tns:consumerEntityType>Car</tns:consumerEntityType>
  
  <tns:annotation>
  	<tns:objects >
  		<tns:object o_name="Location" type="C_CarLocation"/>
  		<tns:object o_name="Progress" type="C_ProgressUnloading"/>
  	</tns:objects>
  	
  	<tns:preconditions>
  		<tns:precondition action="Unload1_Request">
  			<tns:point>
  				<tns:object o_name="Location">
  					<tns:state>Gate1</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  	</tns:preconditions>
  	
  	
  	<tns:effects>
  		<tns:effect action="UnloadFromGate1_Reply">
  			<tns:object o_name="Progress">
  				<tns:event>Completed</tns:event>
  			</tns:object> 		
  		</tns:effect>
  		<tns:effect action="UnloadFromGate1B_Reply">
  			<tns:object o_name="Progress">
  				<tns:event>Completed</tns:event>
  			</tns:object> 		
  		</tns:effect>
  		<tns:effect action="UnloadFromGate1B_Reply">
  			<tns:object o_name="Location">
  				<tns:event>Unloaded2UnloadingArea</tns:event>
  			</tns:object> 		
  		</tns:effect>
  		<tns:effect action="UnloadFromGate1_Reply">
  			<tns:object o_name="Location">
  				<tns:event>Unloaded2UnpackingArea</tns:event>
  			</tns:object> 		
  		</tns:effect>
  	</tns:effects>
  	
  </tns:annotation>
</tns:service>
