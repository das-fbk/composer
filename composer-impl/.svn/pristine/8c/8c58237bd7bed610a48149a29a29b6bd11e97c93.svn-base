<?xml version="1.0" encoding="UTF-8"?>
<tns:service id="STB_BookStorage" xmlns:tns="http://soa.fbk.eu/Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://soa.fbk.eu/Service ../../schemata/Service.xsd ">
  <tns:states>
    <tns:state isInitial="true">ST0</tns:state>
    <tns:state>ST1</tns:state>
    <tns:state>ST2</tns:state>
  </tns:states>
  <tns:actions>
    <tns:input>BookStorageB_Request</tns:input>
    <tns:output>BookedStorageB_Reply</tns:output>
  </tns:actions>
  <tns:transitions>
    <tns:transition>
      <tns:initialState>ST0</tns:initialState>
      <tns:finalState>ST1</tns:finalState>
      <tns:input>BookStorageB_Request</tns:input>
    </tns:transition>
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST2</tns:finalState>
      <tns:output>BookedStorageB_Reply</tns:output>
    </tns:transition>  
  </tns:transitions>
  
  <tns:consumerEntityType>Car</tns:consumerEntityType>
  
  <tns:annotation>
  	<tns:objects >
  		<tns:object o_name="TicketB" type="C_StorageTicketB"/>
  		<tns:object o_name="Type" type="C_CarType"/>
  		<tns:object o_name="AvailB" type="STB_HeavyAvail"/>
  		<tns:object o_name="Info" type="C_StorageInfo"/>
  	</tns:objects>
  	
  	<tns:preconditions>
  		<tns:precondition action="BookStorageB_Request">
  			<tns:point>
  				<tns:object o_name="Type">
  					<tns:state>Heavy</tns:state>
  				</tns:object>
  				<tns:object o_name="AvailB">
  					<tns:state>Yes</tns:state>
  				</tns:object>
  				<tns:object o_name="TicketB">
  					<tns:state>No</tns:state>
  				</tns:object>
  				<tns:object o_name="Info">
  					<tns:state>StorageB</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  	</tns:preconditions>
  	
  	<tns:effects>
  		<tns:effect action="BookedStorageB_Reply">
  			<tns:object o_name="TicketB">
  				<tns:event>Assigned</tns:event>
  			</tns:object> 		
  		</tns:effect>
  	</tns:effects>
  	
  	<tns:compensations>
  		<tns:compensation action="BookedStorageB_Reply">
  			<tns:point>
  				<tns:object o_name="TicketB">
  					<tns:state>No</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:compensation>
  	</tns:compensations>
  	
  
  </tns:annotation>
</tns:service>
