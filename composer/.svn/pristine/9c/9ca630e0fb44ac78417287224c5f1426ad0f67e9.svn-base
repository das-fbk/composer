<?xml version="1.0" encoding="UTF-8"?>
<tns:service id="STB_HeavyStore" xmlns:tns="http://soa.fbk.eu/Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://soa.fbk.eu/Service ../../schemata/Service.xsd ">
  <tns:states>
    <tns:state isInitial="true">ST0</tns:state>
    <tns:state>ST1</tns:state>
    <tns:state>ST2</tns:state>
  </tns:states>
  <tns:actions>
    <tns:input>STB_HeavyStore_Request</tns:input>
    <tns:output>STB_HeavyStored_Reply</tns:output>
  </tns:actions>
  <tns:transitions>
    <tns:transition>
      <tns:initialState>ST0</tns:initialState>
      <tns:finalState>ST1</tns:finalState>
      <tns:input>STB_HeavyStore_Request</tns:input>
    </tns:transition>
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST2</tns:finalState>
      <tns:output>STB_HeavyStored_Reply</tns:output>
    </tns:transition>  
    
  </tns:transitions>
  
  <tns:consumerEntityType>Car</tns:consumerEntityType>
  
  <tns:annotation>
  	<tns:objects >
  		<tns:object o_name="Ticket" type="C_StorageTicketB"/>
  		<tns:object o_name="Type" type="C_CarType"/>
  		<tns:object o_name="Location" type="C_CarLocation"/>
  		<tns:object o_name="Progress" type="C_ProgressStoring"/>
  		<tns:object o_name="Available" type="STB_HeavyAvail"/>
  	</tns:objects>
  	
  	<tns:preconditions>
  		<tns:precondition action="STB_HeavyStore_Request">
  			<tns:point>
  				<tns:object o_name="Ticket">
  					<tns:state>Yes</tns:state>
  				</tns:object>
  				<tns:object o_name="Location">
  					<tns:state>StorageB</tns:state>
  				</tns:object>
  				<tns:object o_name="Type">
  					<tns:state>Heavy</tns:state>
  				</tns:object>
  				<tns:object o_name="Available">
  					<tns:state>Yes</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  	</tns:preconditions>
  	
  	
  	<tns:effects>
  		<tns:effect action="STB_HeavyStored_Reply">
  			<tns:object o_name="Location">
  				<tns:event>StoredAtStorageB</tns:event>
  			</tns:object> 		
  		</tns:effect>
  		<tns:effect action="STB_HeavyStored_Reply">
  			<tns:object o_name="Progress">
  				<tns:event>Completed</tns:event>
  			</tns:object> 		
  		</tns:effect>
  	</tns:effects>
  	
  </tns:annotation>
</tns:service>
