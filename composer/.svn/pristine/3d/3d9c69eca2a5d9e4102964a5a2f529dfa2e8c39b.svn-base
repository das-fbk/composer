<?xml version="1.0" encoding="UTF-8"?>
<tns:service id="SM_DropStorage" xmlns:tns="http://soa.fbk.eu/Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://soa.fbk.eu/Service ../../schemata/Service.xsd ">
  <tns:states>
    <tns:state isInitial="true">ST0</tns:state>
    <tns:state>ST1</tns:state>
    <tns:state>ST2</tns:state>
  </tns:states>
  <tns:actions>
    <tns:input>DropStorage_Request</tns:input>
    <tns:output>DroppedStorage_Reply</tns:output>
  </tns:actions>
  <tns:transitions>
    <tns:transition>
      <tns:initialState>ST0</tns:initialState>
      <tns:finalState>ST1</tns:finalState>
      <tns:input>DropStorage_Request</tns:input>
    </tns:transition>
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST2</tns:finalState>
      <tns:output>DroppedStorage_Reply</tns:output>
    </tns:transition>
  </tns:transitions>
  
  <tns:consumerEntityType>Car</tns:consumerEntityType>
  
  <tns:annotation>
  	<tns:objects >
  		<tns:object o_name="Ticket" type="C_StorageTicket"/>
  	</tns:objects>
  	
  	<tns:preconditions>
  		<tns:precondition action="DropStorage_Request">
  			<tns:point>
  				<tns:object o_name="Ticket">
  					<tns:state>StorageA</tns:state>
  					<tns:state>StorageB</tns:state>
  					<tns:state>StorageC</tns:state>
  					<tns:state>StorageD</tns:state>
  					<tns:state>StorageE</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  	</tns:preconditions>
  	
  	
  	<tns:effects>
  		<tns:effect action="DroppedStorage_Reply">
  			<tns:object o_name="Ticket">
  				<tns:event>Dropped</tns:event>
  			</tns:object> 		
  		</tns:effect>
  	</tns:effects>
  	
  </tns:annotation>
</tns:service>
