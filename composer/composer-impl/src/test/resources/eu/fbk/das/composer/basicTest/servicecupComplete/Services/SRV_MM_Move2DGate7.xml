<?xml version="1.0" encoding="UTF-8"?>
<tns:service id="MM_Move2DGate7" xmlns:tns="http://soa.fbk.eu/Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://soa.fbk.eu/Service ../../schemata/Service.xsd ">
  <tns:states>
    <tns:state isInitial="true">ST0</tns:state>
    <tns:state>ST1</tns:state>
    <tns:state>ST2</tns:state>
  </tns:states>
  <tns:actions>
    <tns:input>Move2DGate7_Request</tns:input>
    <tns:output>Move2DGate7_Reply</tns:output>
  </tns:actions>
  <tns:transitions>
    <tns:transition>
      <tns:initialState>ST0</tns:initialState>
      <tns:finalState>ST1</tns:finalState>
      <tns:input>Move2DGate7_Request</tns:input>
    </tns:transition>
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST2</tns:finalState>
      <tns:output>Move2DGate7_Reply</tns:output>
    </tns:transition>  
  </tns:transitions>
  
  <tns:consumerEntityType>Car</tns:consumerEntityType>
  
  <tns:annotation>
  	<tns:objects >
  		<tns:object o_name="Location" type="C_CarLocation"/>
  		<tns:object o_name="Ticket" type="C_DeliveryTicket"/>
  		<tns:object o_name="Status" type="C_CarStatus"/>
  	</tns:objects>
  	
  	<tns:preconditions>
  		<tns:precondition action="Move2DGate7_Request">
  			<tns:point>
  				<tns:object o_name="Ticket">
  					<tns:state>DGate7</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  		<tns:precondition action="Move2DGate7_Request">
  			<tns:point>
  				<tns:object o_name="Status">
  					<tns:state>Ok</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  		<tns:precondition action="Move2DGate7_Reply">
  			<tns:point>
  				<tns:object o_name="Status">
  					<tns:state>Ok</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  	</tns:preconditions>
  	
  	<tns:effects>
  		<tns:effect action="Move2DGate7_Request">
  			<tns:object o_name="Location">
  				<tns:event>StartedMoving</tns:event>
  			</tns:object>	
  		</tns:effect>
  		<tns:effect action="Move2DGate7_Reply">
  			<tns:object o_name="Location">
  				<tns:event>Moved2DGate7</tns:event>
  			</tns:object> 		
  		</tns:effect>
  	</tns:effects>
  	
  </tns:annotation>
</tns:service>
