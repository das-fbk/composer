<?xml version="1.0" encoding="UTF-8"?>
<tns:service id="G1_Prepare4Large" xmlns:tns="http://soa.fbk.eu/Service" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://soa.fbk.eu/Service ../../schemata/Service.xsd ">
  <tns:states>
    <tns:state isInitial="true">ST0</tns:state>
    <tns:state>ST1</tns:state>
    <tns:state>ST2</tns:state>
  </tns:states>
  <tns:actions>
    <tns:input>G1_Prepare4Large_Request</tns:input>
    <tns:output>G1_Prepare4Large_Reply</tns:output>
  </tns:actions>
  <tns:transitions>
    <tns:transition>
      <tns:initialState>ST0</tns:initialState>
      <tns:finalState>ST1</tns:finalState>
      <tns:input>G1_Prepare4Large_Request</tns:input>
    </tns:transition>
    <tns:transition>
      <tns:initialState>ST1</tns:initialState>
      <tns:finalState>ST2</tns:finalState>
      <tns:output>G1_Prepare4Large_Reply</tns:output>
    </tns:transition>    
  </tns:transitions>
  
  <tns:consumerEntityType>LandingManager</tns:consumerEntityType>
  
  <tns:annotation>
  	<tns:objects >
  		<tns:object o_name="Stat" type="G1_Status"/>
  		<tns:object o_name="Ship" type="S_ShipType"/>
  	</tns:objects>
  	
  	<tns:preconditions>
  		<tns:precondition action="G1_Prepare4Large_Request">
  			<tns:point>
  				<tns:object o_name="Stat">
  					<tns:state>Free</tns:state>
  				</tns:object>
  				<tns:object o_name="Ship">
  					<tns:state>Large</tns:state>
  				</tns:object>
  			</tns:point>
  		</tns:precondition>
  	</tns:preconditions>
  	
  	
  	<tns:effects>
  		<tns:effect action="G1_Prepare4Large_Reply">
  			<tns:object o_name="Stat">
  				<tns:event>ToLanding</tns:event>
  			</tns:object>  		
  		</tns:effect>
  	</tns:effects>
  	
  </tns:annotation>
  
  
</tns:service>